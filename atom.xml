</pre></td><td class='code'><pre><code class='perl'><span class='line'><span class="n">automacro</span> <span class="n">questcheck</span> <span class="p">{</span>
</span><span class='line'>  <span class="n">console</span> <span class="sr">/questcheck (\d+)/</span>
</span><span class='line'>  <span class="n">call</span> <span class="p">{</span>
</span><span class='line'>  <span class="c1">#$qt = questcheck($.lastMatch1)</span>
</span><span class='line'>  <span class="nv">$qt</span> <span class="o">=</span> <span class="n">questtime</span><span class="p">(</span><span class="nv">$</span><span class="err">.</span><span class="nv">lastMatch1</span><span class="p">)</span>
</span><span class='line'>  <span class="nv">$ct</span> <span class="o">=</span> <span class="n">ctime</span><span class="p">(</span><span class="nv">$</span><span class="err">.</span><span class="nv">time</span><span class="p">)</span>
</span><span class='line'>  <span class="k">if</span> <span class="p">(</span><span class="nv">$qt</span> <span class="o">&lt;</span> <span class="nv">$ct</span><span class="p">)</span> <span class="n">stop</span>
</span><span class='line'>  <span class="nb">log</span> <span class="nv">$qt</span> <span class="o">&lt;</span> <span class="nv">$ct</span>
</span><span class='line'>  <span class="n">stop</span>
</span><span class='line'>  <span class="p">}</span>
</span><span class='line'><span class="p">}</span>
</span></code></pre></td></tr></table></div></figure>




